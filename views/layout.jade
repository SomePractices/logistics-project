doctype
html
    head
        meta(charset="utf-8")
        title #{title}
        include ./includes/head
        style.
            #navs{
                display: inline-block;
                float: left;
                width:20%;
                height:100%;
                line-height: 50px;
                text-align: left;
                font-size: 22px;
                background: #3c3c3c;
                padding-top: 50px;
            }
            #main{
                display: inline-block;
                float: left;
                width:70%;
                margin-left: 20px;
            }
    body
        div#navs
            include ./publics/logistics-navs
        div#main
            include ./includes/header
            block content